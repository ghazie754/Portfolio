{
  "name": "my-app",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "next lint && eslint --ext js --ext jsx --ext ts --ext tsx",
    "format": "prettier --write .",
    "silent test-all": "npm --silent run test-all",
    "test-all": "npm run check-lint && npm run check-types && npm run check-format && npm run build"
  },
