    "@types/three": "^0.138.0",
    "@types/webpack": "^5.28.0",
    "@use-gesture/react": "^10.2.10",
    "eslint-webpack-plugin": "^3.1.1",
    "isomorphic-unfetch": "^3.1.0",
    "jshint": "^2.13.4",
    "next": "^12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^8.33.1",
