{
  "name": "my-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "next lint",
    "lint-staged": "npm",
    "format": "prettier --write .",
    "silent test-all": "npm --silent run test-all",
    "test-all": "npm run check-lint && npm run check-types && npm run check-format && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npx eslint --fix",
    "*.{js,jsx,ts,tsx,md,html,css}": "npx prettier --write .",
    "*.css": "stylelint",
    "*.scss": [
      "stylelint --syntax=scss",
      "postcss --config path/to/your/config --replace",
      "stylelint"
    ],
    "*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@react-three/cannon": "^6.3.0",
    "@react-three/drei": "^9.6.2",
    "@react-three/fiber": "^8.0.12",
    "@types/three": "^0.139.0",
    "@types/webpack": "^5.28.0",
    "@use-gesture/react": "^10.2.12",
    "eslint-webpack-plugin": "^3.1.1",
    "isomorphic-unfetch": "^3.1.0",
    "jshint": "^2.13.4",
    "next": "^12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intersection-observer": "^9.1.0",
    "socket.io": "^4.5.0",
    "three": "^0.140.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.7.0",
    "@next/eslint-plugin-next": "^12.1.6",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.174",
    "@swc/jest": "^0.2.20",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.0",
    "lint-staged": ">=12.4.1",
    "netlify-cli": "^10.1.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sass": "^1.51.0",
    "swc-loader": "^0.2.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
