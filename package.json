    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "netlify-cli": "^9.13.3",
    "prettier": "^2.6.0",
