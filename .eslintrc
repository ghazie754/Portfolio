{
  "root": true,
  "extends": [
    "next",
    "google",
    "prettier",
    "next/core-web-vitals",
    "plugin:react/jsx-runtime"
  ],
  "env": {
    "es2020": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    // "react/react-in-jsx-scope": 0,
    // "react/jsx-uses-react": 0,
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  }
}
