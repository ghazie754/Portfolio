{
  "root": true,
  "extends": [
    "next",
    "google",
    "prettier",
    "plugin:@next/next/recommended",
    "next/core-web-vitals"
  ],
  "env": {
    "es2020": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["@next/next"]
}
